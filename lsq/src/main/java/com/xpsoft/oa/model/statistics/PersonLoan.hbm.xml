<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.statistics.PersonLoan"
        table="person_loan"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="loanReport"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loanReport"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="loanDepatment"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loanDepatment"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="personCompanyName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="personCompanyName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="loanMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="loanMoney"
            length="15"
            not-null="false"
            unique="false"
        />
        <property
            name="loanYear"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loanYear"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="loanMonth"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loanMonth"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="loanCase"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loanCase"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="loanMoneyBig"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loanMoneyBig"
            length="120"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
