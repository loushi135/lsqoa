<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.hrm.TrainApply"
        table="train_apply"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        <property
            name="trainTarget"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="train_target"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="trainType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="train_type"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="publish"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="publish"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <many-to-one 
	        name="applyUser" 
	        column="applyUser" 
	        class="com.xpsoft.oa.model.system.AppUser" 
	        fetch="join"
	        not-null="true"
        >
        </many-to-one>
        <many-to-one 
	        name="department" 
	        column="department" 
	        class="com.xpsoft.oa.model.system.Department" 
	        fetch="join"
	        not-null="true"
        >
        </many-to-one>
        <many-to-one 
	        name="trainCourse" 
	        column="train_course" 
	        class="com.xpsoft.oa.model.hrm.TrainCourse" 
	        fetch="join"
	        not-null="true"
        >
        </many-to-one>
     </class>
</hibernate-mapping>
