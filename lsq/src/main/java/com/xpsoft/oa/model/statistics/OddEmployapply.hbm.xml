<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.statistics.OddEmployapply"
        table="odd_employapply"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        <many-to-one name="contract" class="com.xpsoft.oa.model.statistics.MaterialContract" not-null="false" fetch="join" not-found="ignore" >
        	<column name="contractId"></column>
        </many-to-one>
        <property
            name="employType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="employType"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="num"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="num"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="contact"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="contact"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="employTime"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="employTime"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="employFee"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="employFee"
            precision="10"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="employReason"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="employReason"
            length="65535"
            not-null="false"
            unique="false"
        />
        <property
            name="employContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="employContent"
            length="65535"
            not-null="false"
            unique="false"
        />
        <property
            name="attachIds"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="attachIds"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="attachFiles"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="attachFiles"
            length="5000"
            not-null="false"
            unique="false"
        />
        <property
            name="processRunId"
            type="java.lang.Long"
            update="false"
            insert="true" 
            access="property"
            column="processRunId"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
