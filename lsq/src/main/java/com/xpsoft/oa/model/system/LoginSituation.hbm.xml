<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.system.LoginSituation"
        table="login_situation"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="message"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="message"
            length="500"
            not-null="true"
            unique="false"
        />
        <property
            name="userId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="userId"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="userName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="userName"
            length="500"
            not-null="true"
            unique="false"
        />
        <property
            name="ip"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ip"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="actionTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="actionTime"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
