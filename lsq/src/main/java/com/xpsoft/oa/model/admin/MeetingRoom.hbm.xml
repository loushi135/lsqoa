<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.admin.MeetingRoom"
        table="meeting_room"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="maxPerson"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="maxPerson"
            length="10"
            not-null="true"
            unique="false"
        />
        <property
            name="microphone"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="microphone"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="speaker"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="speaker"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="projector"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="projector"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="znhyt"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="znhyt"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="notebook"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="notebook"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="wordPad"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="wordPad"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="status"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remark"
            length="500"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
