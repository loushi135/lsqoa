<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.flow.TaskAgent"
        table="task_agent"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="assignId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="assignId"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="assignName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="assignName"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="agentAssignId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="agent_assignId"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="agentAssignName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="agent_assignName"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="status"
            length="10"
            not-null="true"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remark"
            length="512"
            not-null="false"
            unique="false"
        />
        <property
            name="optDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="optDate"
            length="19"
            not-null="true"
            unique="false"
        />
     </class>
</hibernate-mapping>
