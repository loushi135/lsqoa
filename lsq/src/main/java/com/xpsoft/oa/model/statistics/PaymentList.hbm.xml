<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.statistics.PaymentList"
        table="payment_list"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        <many-to-one name="project"
         class="com.xpsoft.oa.model.statistics.ProjectNew" 
         not-null="false"
         fetch="join">
        	<column name="proId"></column>
        </many-to-one>
        
        <many-to-one name="user"
         class="com.xpsoft.oa.model.system.AppUser" 
         not-null="false"
         fetch="join">
        	<column name="borrowerId"></column>
        </many-to-one>
        
        
        <property
            name="deptName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="deptName"
            length="80"
            not-null="false"
            unique="false"
        />
        <property
            name="paymentSumBig"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="paymentSumBig"
            length="128"
            not-null="false"
            unique="false"
        />
          <property
            name="payOption"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="payOption"
            length="128"
            not-null="false"
            unique="false"
        />
          <property
            name="paymentCause"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="paymentCause"
            length="1000"
            not-null="false"
            unique="false"
        />
        <property
            name="paymentSumSmall"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="paymentSumSmall"
            length="10"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="preNowReturnTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="preNowReturnTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="owedSum"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="owedSum"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="preOwedReturnTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="preOwedReturnTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="borrower"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="borrower"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="paymentType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="paymentType"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="processRunId"
            type="java.lang.Long"
            update="false"
            insert="true" 
            access="property"
            column="processRunId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="attachIds"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="attachIds"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="attachFiles"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="attachFiles"
            length="5000"
            not-null="false"
            unique="false"
        />
        <property
            name="payMoneyDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="payMoneyDate"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
