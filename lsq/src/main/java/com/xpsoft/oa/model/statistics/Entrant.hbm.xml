<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.statistics.Entrant"
        table="entrant"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="entrantId"
            column="entrantId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="entrantType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="entrantType"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="signTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="signTime"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="position"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="position"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="inviteWay"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="inviteWay"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="inviteReason"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="inviteReason"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="professional"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="professional"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="experience"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="experience"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="confident"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="confident"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="signName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="signName"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="thinkability"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="thinkability"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="expressability"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="expressability"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="looks"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="looks"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="inspect"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="inspect"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="inspectSalary"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="inspectSalary"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="probation"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="probation"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="probationSalary"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="probationSalary"
            length="10"
            not-null="false"
            unique="false"
        />
        <set name="entrantFiles"
	     table="entrantFile"
	     lazy="true"
	     inverse="false"
	     cascade="save-update"
	     sort="unsorted"
		>
		<key>
			<column name="entrantId"/>
		</key>
		<many-to-many fetch="join" class="com.xpsoft.oa.model.system.FileAttach" column="fileId"/>
	</set>
     </class>
</hibernate-mapping>
