<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.hrm.TrainSurveyUser"
        table="train_survey_user"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <many-to-one 
         	name="trainSurvey" 
         	class="com.xpsoft.oa.model.hrm.TrainSurvey"
         	fetch="join"
         	column="trainSurveyId">
         	</many-to-one>
        <property
            name="userId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="userId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="voted"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="voted"
            length="2"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
