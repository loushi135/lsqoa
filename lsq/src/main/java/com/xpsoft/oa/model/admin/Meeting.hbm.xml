<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.admin.Meeting"
        table="meeting"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="meetingNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="meetingNo"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="department"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="department"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="applyUser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="applyUser"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="userId"
            update="true"
            insert="true"
            access="property"
            column="userId"
            type="java.lang.Long"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="startTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="startTime"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="entTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="entTime"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="meetingTitle"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="meetingTitle"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="personNum"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="personNum"
            length="10"
            not-null="true"
            unique="false"
        />
        <property
            name="meetingType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="meetingType"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="meetingRoom"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="meetingRoom"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="meetingRequire"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="meetingRequire"
            length="1000"
            not-null="false"
            unique="false"
        />
        <property
            name="processRunId"
            type="java.lang.Long"
            update="false"
            insert="true" 
            access="property"
            column="processRunId"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
