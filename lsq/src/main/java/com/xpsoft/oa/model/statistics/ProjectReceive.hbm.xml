<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.statistics.ProjectReceive"
        table="project_receive"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="amountBig"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="amountBig"
            length="128"
            not-null="false"
            unique="false"
        />
                 <property
            name="pzzh"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="pzzh"
            length="128"
            not-null="false"
            unique="false"
        />
                 <property
            name="zy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="zy"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="amount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="amount"
            precision="10"
            scale="2"
            not-null="false"
            unique="false"
        />
        <many-to-one
			name="project"
			class="com.xpsoft.oa.model.statistics.ProjectNew"
            not-null="false"
			fetch="join" lazy="false"
		>
			<column name="proId" />
		</many-to-one>
        <property
            name="receiveTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="receiveTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="createtime"
            type="java.util.Date"
            update="false"
            insert="true"
            access="property"
            column="createtime"
            length="19"
            not-null="false"
            unique="false"
        />
		<many-to-one
			name="createUser"
			class="com.xpsoft.oa.model.system.AppUser"
            not-null="false"
			fetch="select"
			insert="true"
			update="false"
		>
			<column name="createUser" />
		</many-to-one>
     </class>
</hibernate-mapping>
