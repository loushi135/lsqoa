<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.flow.ProSubjectDef"
        table="pro_subject_def"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="defId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="defId"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="refField"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="refField"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="defaultVal"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="defaultVal"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="refLabel"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="refLabel"
            length="128"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
