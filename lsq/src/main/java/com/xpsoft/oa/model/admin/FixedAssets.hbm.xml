<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.admin.FixedAssets"
        table="fixed_assets"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="assetsId"
            column="assetsId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="assetsNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="assetsNo"
            length="128"
            not-null="false"  
            unique="false"
        />
        <property
            name="assetsName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="assetsName"
            length="128"
            not-null="true"
            unique="false"
        />
        <property
            name="model"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="model"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="manufacturer"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="manufacturer"
            length="64"
            not-null="false"
            unique="false"
        />       
        <property
            name="buyDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="buyDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="beDep"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="beDep"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="custodian"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="custodian"
            length="32"
            not-null="true"
            unique="false"
        />
        <property
            name="notes"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="notes"
            length="500"
            not-null="false"
            unique="false"
        />
      	<property
            name="configuration"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="configuration"
            length="128"
            not-null="false"
            unique="false"
        />
       <property
            name="sn"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="sn"
            length="64"
            not-null="false"
            unique="false"
        />
      <property
            name="assetscost"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="assetscost"
            length="18"
            not-null="false"
            unique="false"
        />
     	<property
            name="user"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="user"
            length="64"
            not-null="false"
            unique="false"
        />
        <property
            name="barcode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="barcode"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="fileindex"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="fileindex"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="inperson"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="inperson"
            length="64"
            not-null="false"
            unique="false"
        />
         <property
            name="labelStatus"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="labelStatus"
            length="6"
            not-null="true"
            unique="false"
        />
          <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="status"
            length="6"
            not-null="true"
            unique="false"
        />
		
    <!-- imported bi-directional many-to-one association to AssetsType -->
		<many-to-one
			name="assetsType"
			class="com.xpsoft.oa.model.admin.AssetsType"
            not-null="false"
			fetch="select"
		>	
			<column name="assetsTypeId" />
		</many-to-one>
 
    </class>
</hibernate-mapping>
