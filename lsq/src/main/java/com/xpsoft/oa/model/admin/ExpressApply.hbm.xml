<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.admin.ExpressApply"
        table="express_apply"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="applyer"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="applyer"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="applyerId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="applyerId"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="applyDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="applyDate"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="expressType"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="expressType"
            length="10"
            not-null="true"
            unique="false"
        />
         <property
            name="provinceId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="provinceId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="province"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="province"
            length="255"
            not-null="false"
            unique="false"
        />
         <property
            name="cityId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="cityId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="city"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="city"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="toWhereName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="toWhereName"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="expressName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="expressName"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="expressNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="expressNo"
            length="255"
            not-null="false"
            unique="false"
        />
         <property
            name="processRunId"
            type="java.lang.Long"
            update="false"
            insert="true"
            access="property"
            column="processRunId"
            length="19"
            not-null="false"
            unique="false"
        />
        <many-to-one name="dept" 
        	class="com.xpsoft.oa.model.system.Department"
			not-null="false" fetch="join">
			<column name="deptId"></column>
		</many-to-one>
		<many-to-one name="projectNew" 
        	class="com.xpsoft.oa.model.statistics.ProjectNew"
			not-null="false" fetch="join">
			<column name="proId"></column>
		</many-to-one>
     </class>
</hibernate-mapping>
