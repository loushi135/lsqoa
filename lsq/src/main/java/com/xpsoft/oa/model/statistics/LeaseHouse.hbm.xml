<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.statistics.LeaseHouse"
        table="lease_house"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="reporter"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="reporter"
            length="120"
            not-null="false"
            unique="false"
        />
        <property
            name="reporterDepatment"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="reporterDepatment"
            length="120"
            not-null="false"
            unique="false"
        />
        <property
            name="reporterCompanyName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="reporterCompanyName"
            length="120"
            not-null="false"
            unique="false"
        />
        <property
            name="sort"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="sort"
            length="120"
            not-null="false"
            unique="false"
        />
        <many-to-one name="project"
         class="com.xpsoft.oa.model.statistics.ProjectNew" 
         not-null="false"
         fetch="join">
        	<column name="projectId"></column>
        </many-to-one>
        <many-to-one name="projectManager"
         class="com.xpsoft.oa.model.system.AppUser" 
         not-null="false"
         fetch="join">
        	<column name="projectManagerId"></column>
        </many-to-one>
        <property
            name="leaseStart"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="leaseStart"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="leaseEnd"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="leaseEnd"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="monthlyRent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="monthlyRent"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="yearRent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="yearRent"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="leaseLen"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="leaseLen"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="cause"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="cause"
            length="65535"
            not-null="false"
            unique="false"
        />
        <property
            name="processRunId"
            type="java.lang.Long"
            update="false"
            insert="true" 
            access="property"
            column="processRunId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="attachIds"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="attachIds"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="attachFiles"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="attachFiles"
            length="5000"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
