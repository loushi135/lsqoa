            unique="false"
        />
        <property
            name="inspectSalary"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="inspectSalary"
            length="15"
            not-null="false"
            unique="false"
        />
        <property
            name="probation"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="probation"
            length="15"
            not-null="false"
            unique="false"
        />
        <property
            name="probationSalary"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
