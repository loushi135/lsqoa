            name="medicalCheck"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="medicalCheck"
            length="512"
            not-null="false"
            unique="false"
        />
        <property
            name="score"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="score"
            length="128"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
