<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.statistics.StaffEmployapply"
        table="staff_employapply"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="staffName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="staffName"
            length="128"
            not-null="false"
            unique="false"
        />
        
        <many-to-one name="dept" 
        	class="com.xpsoft.oa.model.system.Department"
			not-null="false" fetch="join">
			<column name="deptId"></column>
		</many-to-one>
		
		<many-to-one name="job" 
        	class="com.xpsoft.oa.model.hrm.Job"
			not-null="false" fetch="join">
			<column name="jobId"></column>
		</many-to-one>
		
        <property
            name="inviteWay"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="inviteWay"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="publicWay"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="publicWay"
            length="128"
            not-null="false"
            unique="false"
        />
        
        <many-to-one name="inviteUser" 
        	class="com.xpsoft.oa.model.system.AppUser"
			not-null="false" fetch="join">
			<column name="inviteUserId"></column>
		</many-to-one>

        <property
            name="inviteReason"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="inviteReason"
            length="65535"
            not-null="false"
            unique="false"
        />
        <property
            name="attachIDs"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="attachIDs"
            length="512"
            not-null="false"
            unique="false"
        />
        <property
            name="attachFile"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="attachFile"
            length="5000"
            not-null="false"
            unique="false"
        />
        <property
            name="professional"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="professional"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="experience"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="experience"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="confident"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="confident"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="thinkability"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="thinkability"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="expressability"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="expressability"
            length="255"
            not-null="true"
            unique="false"
        />
        <property
            name="looks"
            type="java.lang.String"
            update="true"
            insert="true"
