            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="proAddr"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="proUseful"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="proUseful"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="proMainUnit"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="proMainUnit"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="owerRelation"
            type="java.lang.String"
            update="true"
