        <property
            name="bidDuringTime"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bidDuringTime"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="progressInfo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="progressInfo"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="chargeDesign"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="chargeDesign"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="doReview"
            type="java.lang.String"
            update="true"
