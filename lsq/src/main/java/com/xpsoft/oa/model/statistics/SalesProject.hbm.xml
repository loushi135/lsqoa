<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.statistics.SalesProject"
        table="sales_project"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="proNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="proNo"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="proName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="proName"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remark"
            length="65535"
            not-null="false"
            unique="false"
        />
        
        <many-to-one
			name="teamDep"
			class="com.xpsoft.oa.model.system.Department"
            not-null="false"
			fetch="join"
		>
			<column name="teamDepId" />
		</many-to-one>
		
		<many-to-one
			name="businessUser"
			class="com.xpsoft.oa.model.system.AppUser"
            not-null="false"
			fetch="join"
		>
			<column name="businessUserId" />
		</many-to-one>
		<property
            name="applyDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="applyDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="buildPrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="buildPrice"
            precision="13"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="buildArea"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="buildArea"
            precision="13"
            scale="2"
            not-null="false"
            unique="false"
        />
        <property
            name="proInfoSource"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="proInfoSource"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
