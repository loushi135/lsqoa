            column="recommendDesignDept"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="proClassify"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="proClassify"
            length="10"
            not-null="false"
