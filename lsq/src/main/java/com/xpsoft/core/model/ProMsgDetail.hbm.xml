<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.core.model.ProMsgDetail"
        table="pro_msg_detail"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="typeId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="type_id"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="strId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="str_id"
            length="100"
            not-null="true"
            unique="false"
        />
        <property
            name="processId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="process_id"
            length="128"
            not-null="true"
            unique="false"
        />
        <property
            name="taskId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="task_id"
            length="128"
            not-null="true"
            unique="false"
        />
        <property
            name="taskName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="task_name"
            length="128"
            not-null="true"
            unique="false"
        />
        <property
            name="userId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="user_id"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="sucRes"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="suc_res"
            length="50"
            not-null="true"
            unique="false"
        />
        <property
            name="faiRes"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="fai_res"
            length="50"
            not-null="true"
            unique="false"
        />
     </class>
</hibernate-mapping>
