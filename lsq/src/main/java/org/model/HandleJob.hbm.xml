<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="org.model.HandleJob"
        table="handle_job"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        <property
            name="sourceApp"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="sourceApp"
            length="128"
            not-null="false"
            unique="false"
        />
       
        <property
            name="billCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="billCode"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="waitHandleJobId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="waitHandleJobId"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="workflowName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="workflowName"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="requestJobCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="requestJobCode"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="handlerJobCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="handlerJobCode"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="workflowNodeName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="workflowNodeName"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="waitHandleJobUrl"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="waitHandleJobUrl"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="factHandleJobCode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="factHandleJobCode"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="requestMethod"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="requestMethod"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="runId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="runId"
            length="128"
            not-null="false"
            unique="false"
        />        
     </class>
</hibernate-mapping>
